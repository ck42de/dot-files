[alias]

  a = add
  b = branch
  c = commit
  d = diff
  f = fetch
  g = grep
  l = log
  m = merge
  o = checkout
  p = pull
  r = remote
  s = status
  w = whatchanged

  # Checkout
  co = checkout
  cob = checkout -b

  # Commit 
  cm = commit --message
  cam = commit --all --message

  # Log
  lp = log -p
  lo = log --oneline
  lpr = log --pretty=format:'%C(yellow)%h %Cred%ad %C(bold blue)%an%Cgreen%d %Creset%s' --relative-date
  lcgp = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit

  # Misc
  acp = !git add --all && git commit -m `date "+%Y-%m-%d"` && git push

  # Push and Pull
  pl = pull

  # Status
  sb = status -sb
