[alias]

  a = add
  b = branch
  c = commit
  d = diff
  f = fetch
  g = grep
  l = log
  m = merge
  o = checkout
  p = pull
  r = remote
  s = status
  w = whatchanged

  # Checkout
  co = checkout
  cob = checkout -b

  # Log
  lp = log -p

  # Misc
  acp = !git add --all && git commit -m `date "+%Y-%m-%d"` && git push

  # Push and Pull
  pu = push
  pl = pull

  # Status
  sb = status -sb
